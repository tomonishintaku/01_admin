.post blockquote p {
	margin-bottom: 0;
}
.post blockquote.tw {
	background-image: url(/blog/wp-content/themes/stinger7/images/quote-tw.png);
	padding-top: 3.5rem;
}
.fa-clock-o, .fa-tags {
	color: #aaa;
}
.post p img {
	display: block;
	width: 70%;
	margin: 0 auto 2.5em auto;
}
.post p img.art-pic {
	display: block;
	width: 100%;
	margin: 1em auto 0 auto;
}
.post p img.doc-pic {
	display: block;
	width: 95%;
}
.post .overview {
	border: 1px solid #ccc;
	padding: 1em;
	margin-bottom: 1.5em;
}
.post .overview ul {
	list-style: none;
	padding: 0;
	margin: 0;
}
.post .overview li {
	border-bottom: 1px dotted #ccc;
	padding: 0.5em 0;
}
.post .overview li:last-child {
	border: none;
	padding-bottom: 0;
}
.post, .post p {
	line-height: 1.8;
}
.post img.width-100 {
	width: 100%;
}
.post div a img {
	display: block;
	width: 60%;
	margin: 0 auto 2.5em auto;
}
.post .abh_box img {
	width: 100%;
}
.p-navi dl dt {
	white-space: nowrap;
	padding-right: 3em;
}
.p-navi dl dt i {
	padding: 0 0.2em;
}
#comments h3 {
	color: #ff1493;
	font-weight: normal;
}
#comments {
	padding: 1.5em 0 0 0;
}
.comment-respond textarea {
	height: 4em;
}
#pagetop {
	font-size: 14px;
}
.sns .fa {
	padding: 0 0 0 2px;
	width: 18px;
}
.sns .fa-hatena {
	width: auto;
}
.st-pagelink .current {
	color: #ddd;
}
.sns i {
	padding: 0!important;
}
.abh_tab_content {
	padding-bottom: 0!important;
}
.no-thumbitiran {
	border-bottom: 1px dotted #ccc;
	margin-bottom: 1.5em;
}
.no-thumbitiran:last-child {
	border-bottom: none;
}
@media only screen and (min-width: 481px) {
	body {
		width: 100%;
		margin-top: 40px!important;
		padding: 0;
	}
	h1.descr {
		font-size: 12px;
		color: #616161;
		margin-top: 40px;
		margin-bottom: -60px;
	}
	h1.descr, p.descr {
		width: 650px!important;
		font-size: 1rem;
		position: relative;
		top: 0.9rem;
		text-align: right;
	}
	.single h1.descr, .single p.descr, .archive p.descr, .archive p.descr {
		top: -6px;
	}
	body.single, body.archive {
		margin-top: 60px!important;
	}
	#head h2#logotype {
		margin-top: -20px!important;
	}
	#head {
		height: 10px!important;
		margin: 64px auto -30px auto!important;
	}
	div#gnav {
		margin-top: 60px;
	}
	.text-pc-center {
		text-align: center;
	}
	.post div.art-pic-half {
		display: block;
		width: 50%;
		margin: 0 auto;
	}
	.post p img.art-pic-half {
		display: block;
		width: 50%;
		margin: 1em auto 0 auto;
	}
	.abh_tab_content .abh_text .abh_description {
		margin-bottom: 0;
	}
}
@media only screen and (max-width: 780px) and (min-width: 320px) {
	main {
		padding: 10px!important;
	}
}
@media only screen and (max-width: 480px) and (min-width: 0px) {
	.post p img.art-pic-half {
		width: 100%;
	}
	main {
		padding: 20px 0;
		margin: 0 auto;
	}
	#wrapper {
		width: 100%;
	}
	.android h1.descr, .android p.descr {
		padding: 3.5em 0.5em 0 0.5em;
	}
	.iphone h1.descr, .iphone p.descr {
		padding: 3.5em 0.5em 0 0.5em;
	}
	h1.descr, p.descr {
		font-size: 12px;
		padding: 0 0.5em 0 0.5em;
		border-left: none;
	}
	.abh_tab_content {
		padding: 10px 12px !important;
	}
	.post h4.point {
		margin-top: -2em;
	}
	.p-navi {
		background: #fbfbfb;
		padding: 0.5em;
		margin: 0 auto 1em auto;
	}
	.p-navi dl dd, .p-navi dl dt {
		padding-top: 0.5em;
		padding-bottom: 0.5em;
	}
	.post .abh_box {
		margin: 0 auto;
	}
	#comments {
		padding: 0;
		margin: 0 auto;
	}
	.kanren .smanone2 p {
		line-height: 1.2em;
		font-size: 1em;
	}
	.kanren h5 {
		line-height: 1.5em;
		font-size: 1.2em;
	}
	.dl-menu .sns {
		margin-bottom: 0;
	}
	.dl-menu .sns i {
		color: #777!important;
	}
	.p-navi dl dd {
		line-height: 1.2em;
	}
	.lang-switch .common-nav {
		display: none!important;
	}
	.lang-switch .blog-nav {
		display: block!important;
	}
}

